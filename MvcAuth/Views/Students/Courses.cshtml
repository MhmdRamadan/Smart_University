@model IEnumerable<MvcAuth.Models.RegisterCoursesTbl>

@{
    ViewBag.Title = "Courses";
}

<h2>Courses</h2>

<p>
    @Html.ActionLink("Register New Course", "RegisterCourse")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course_ID)
        </th>

    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CourseTbl.Course_Name)
        </td>
    </tr>
}

</table>
